<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.keeps_000.clearer.activity.TeamInfoActivity">

    <RelativeLayout
        android:background="@mipmap/main_background1"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <Button
            android:id="@+id/btn_back_5"
            android:drawableLeft="@drawable/btn_back"
            android:background="#00000000"
            android:layout_width="100dp"
            android:layout_height="45dp" />

        <TextView
            android:id="@+id/text_title_kkk"
            android:text="团队介绍"
            android:textSize="45px"
            android:textStyle="bold"
            android:gravity="center"
            android:layout_width="match_parent"
            android:layout_height="45dp" />

        <ListView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_below="@+id/text_title_kkk"
            android:id="@+id/team_info_list_view"
            >
        </ListView>

    </RelativeLayout>

</android.support.constraint.ConstraintLayout>
